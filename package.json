{
  "name": "clase2",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --config=jest/jest.config.js --detectOpenHandles",
    "test:watch": "jest --watch --verbose false",
    "lint:js": "npx eslint src",
    "lint:js:fix": "npx eslint src --fix",
    "lint:report": "npx eslint src -o ./eslint-report.json --format json",
    "sonar:scan": "node sonarScanner",
    "clean-node": "rm -rf node_modules/ && yarn install",
    "clean-pods": "rm -rf ios/Pods/ && npx pod-install",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "expo": "~50.0.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "v8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "jest": "^29.3.1",
    "husky": "^9.0.6",
    "prettier": "3.2.4"
  },
  "private": true
}